<?xml version="1.0"?>
<robot name="robocam">

    <!-- Robot Links -->
    <link name="base">
        <visual>
            <geometry>
                <cylinder length=".08" radius="0.03"/>
            </geometry>
            <origin xyz="0 0 .04" rpy="0 0 0" />
        </visual>
    </link>

    <link name="link_rot">
        <visual>
            <geometry>
                <cylinder length=".04" radius="0.03"/>
            </geometry>
            <origin xyz="0 0 .02" rpy="0 0 0" />
            <material name='blue'>
                <color rgba="0 0 0.8 1"/>
            </material>
        </visual>
    </link>
    
    <link name="link_1">
        <visual>
            <geometry>
                <cylinder length=".32" radius="0.03"/>
            </geometry>
            <origin xyz="0 0 .16" rpy="0 0 0" />
        </visual>
    </link>

    <link name="link_f1">
        <visual>
            <geometry>
                <cylinder length=".07" radius="0.03"/>
            </geometry>
            <origin xyz="0 0 .035" rpy="0 0 0" />
            <material name='black'>
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
    </link>

    <link name="link_2">
        <visual>
            <geometry>
                <cylinder length=".32" radius="0.03"/>
            </geometry>
            <origin xyz="0 0 .16" rpy="0 0 0" />
            <material name='blue'>
                <color rgba="0 0 0.8 1"/>
            </material>
        </visual>
    </link>

    <link name="link_f2">
        <visual>
            <geometry>
                <cylinder length=".05" radius="0.03"/>
            </geometry>
            <origin xyz="0 0 .025" rpy="0 0 0" />
            <material name='black'>
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
    </link>

    <link name="link_pitch">
        <visual>
            <geometry>
                <cylinder length=".05" radius="0.03"/>
            </geometry>
            <origin xyz="0 0 .025" rpy="0 0 0" />
        </visual>
    </link>
    
    <link name="link_yaw">
        <visual>
            <geometry>
                <cylinder length=".02" radius="0.02"/>
            </geometry>
            <origin xyz="0 0 .01" rpy="0 0 0" />
            <material name='blue'>
                <color rgba="0 0 0.8 1"/>
            </material>
        </visual>
    </link>

    <link name="link_roll">
        <visual>
            <geometry>
                <cylinder length=".02" radius="0.03"/>
            </geometry>
            <origin xyz="0 0 .01" rpy="0 0 0" />
        </visual>
    </link>

    <!-- Robot Joints -->
    <joint name="joint_base_rot" type="continuous">
        <parent link="base"/>
        <child link="link_rot"/>
        <origin xyz="0 0 .08" rpy="0 0 0" />
        <axis xyz="0 0 1" />
    </joint>

  
    <joint name="joint_rot_1" type="continuous">
        <parent link="link_rot"/>
        <child link="link_1"/>
        <origin xyz="0 0 .04" rpy="-0.2507 0 0" />
        <axis xyz="1 0 0" />
    </joint>

    <joint name="joint_1_f1" type="fixed">
        <parent link="link_1"/>
        <child link="link_f1"/>
        <origin xyz="0 0 .32" rpy="-0.4720 0 0" />
    </joint>

    <joint name="joint_f1_2" type="continuous">
        <parent link="link_f1"/>
        <child link="link_2"/>
        <origin xyz="0 0 .04" rpy="0 0 0" />
        <axis xyz="1 0 0" />
    </joint>

    <joint name="joint_2_f2" type="fixed">
        <parent link="link_2"/>
        <child link="link_f2"/>
        <origin xyz="0 0 .32" rpy="-1.1139 0 0" />
    </joint>

    <joint name="joint_f2_pitch" type="continuous">
        <parent link="link_f2"/>
        <child link="link_pitch"/>
        <origin xyz="0 0 0.05" rpy="-.5163 0 0" />
        <axis xyz="1 0 0" />
    </joint>

    <joint name="joint_pitch_yaw" type="continuous">
        <parent link="link_pitch"/>
        <child link="link_yaw"/>
        <origin xyz="0 0.0225 .05" rpy="0 0 0" />
        <axis xyz="0 0 1" />
    </joint>

    <joint name="joint_yaw_roll" type="continuous">
        <parent link="link_yaw"/>
        <child link="link_roll"/>
        <origin xyz="0 0 0.05" rpy="0 1.57 0" />
        <axis xyz="0 0 1" />
    </joint>
</robot>

